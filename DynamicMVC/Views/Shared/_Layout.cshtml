<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dynamic MVC</title>
    <link href="~/content/initial.css" rel="stylesheet" />
    <link href="~/content/basics.css" rel="stylesheet" />
    <script src="~/js/jquery.js"></script>
</head>
<body>
    <div id="outer">
        <div id="hdr">
            <h1>Dynamic MVC by CABE</h1>
        </div>
        <div id="nav">
            <h2>Navigation</h2>
            <ul>
                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-controller="Challenge" asp-action="One">Challenge 1</a></li>
                <li><a asp-controller="Challenge" asp-action="Two">Challenge 2</a></li>
                <li><a asp-controller="Challenge" asp-action="Three">Challenge 3</a></li>
                <li><a asp-controller="Challenge" asp-action="Four">Challenge 4</a></li>
                <li><a asp-controller="Challenge" asp-action="Five">Challenge 5</a></li>
            </ul>
        </div>
        <div id="cont">
            @RenderBody()
            @RenderSection("scripts", required: false)
        </div>
        <div id="ftr">CABE @DateTime.Now.Year</div>
    </div>
</body>
</html>




